{
    "name": "charcount",
    "displayName": "CharCount",
    "description": "Markdown Char Count",
    "version": "0.0.1",
    "publisher": "m0t0k1",
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.charCount",
        "onLanguage:markdown"
